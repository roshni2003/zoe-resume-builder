{
  "buildCommand": "pnpm build && cp -r migrations .output/server/ || true",
  "outputDirectory": ".output/public",
  "framework": null,
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "functions": {
    "api/**/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
